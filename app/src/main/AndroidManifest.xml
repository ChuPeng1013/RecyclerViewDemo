<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.administrator.recyclerview">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".linearLayoutVerticalActivity"></activity>
        <activity android:name=".linearLayoutHorizontalActivity"></activity>
        <activity android:name=".gridLayoutVerticalActivity"></activity>
        <activity android:name=".gridLayoutHorizontalActivity"></activity>
        <activity android:name=".staggeredGridLayoutVerticalActivity"></activity>
        <activity android:name=".staggeredGridLayoutHorizontalActivity"></activity>
    </application>

</manifest>